#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int n,q,s,va;
    cin>>n>>q;
    int a[n],qe[q][2];
    vector<vector<int>> v;
    for(int i=0;i<n;i++){
      cin>>s;
      vector<int> v1;
      for(int j=0;j<s;j++){
          cin>>va;
          v1.push_back(va);
      } 
      v.push_back(v1);
    }
    for(int i=0;i<q;i++){
       for(int j=0;j<2;j++){
           cin>>qe[i][j];
       }
    }
    for(int i=0;i<q;i++){
        
        cout<<v[qe[i][0]][qe[i][1]]<<"\n";
    }
    return 0;
}

